{# user page link to user's order #}
{% extends 'base.jinja' %}
{% block content %}

<div class="userlist">
    {% for user in users %}
    <ul style="list-style-type:none;">
        <a href="/users/{{user.id}}/my_orders"><li><p class="text">{{user.name}}</p></li></a>
    </ul>
    {% endfor %}
</div>
<span class="new_user_button"><button onclick="location.href='/users/add';" class="buttons">Create New Account</button></span>
{% endblock content %}